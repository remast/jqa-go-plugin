<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="PLSQL">
    <description>Provides a scanner for PLSQL files.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.go.model.GoDescriptor</class>
        <class>org.jqassistant.contrib.plugin.go.model.GoFileDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.go.scanner.GoFileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>